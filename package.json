{
  "name": "@qiwi/toposort",
  "version": "1.0.3",
  "description": "Topological sort of directed ascyclic graphs (like dependecy lists)",
  "main": "dist/main/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublish": "tsc",
    "style": "eslint src",
    "style:fix": "yarn style --fix",
    "verify": "yarn style && yarn test",
    "test": "yarn test:unit",
    "test:unit": "c8 -r html -r text -r lcov --exclude src/test uvu -r tsm src/test",
    "test:unit2": "tsc && c8 -r html -r text -r lcov --exclude dist/test uvu dist/test -i \\d+.ts"
  },
  "type": "module",
  "types": "dist/main/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiwi-forks/toposort.git"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "c8": "^7.12.0",
    "eslint": "^8.32.0",
    "eslint-config-qiwi": "^2.0.8",
    "tsm": "^2.3.0",
    "typescript": "^4.9.4",
    "uvu": "^0.5.6"
  },
  "keywords": [
    "topological",
    "sort",
    "sorting",
    "graphs",
    "graph",
    "dependency",
    "list",
    "dependencies",
    "acyclic"
  ],
  "author": "Marcel Klehr <mklehr@gmx.net>",
  "license": "MIT"
}
